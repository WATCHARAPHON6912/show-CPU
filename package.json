{
  "name": "gpu-usage-graph",
  "displayName": "GPU Usage Graph",
  "description": "Show GPU usage graph in the activity bar",
  "version": "0.0.1",
  "publisher": "nvidia-monitor",
  "icon": "logo.png",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "views": {
      "activitybar": [
        {
          "type": "webview",
          "id": "gpuUsageGraph",
          "name": "CPU Graph"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  }
}